#include <iostream>
using namespace std;
int main(){

	int i =9;
	int *p = &i;
	int *q =p;
	cout<<*q<<endl;
	(*q)++;
	cout<<i<<endl;
	
	// &-- address of
	// *--Location of 
	
	//char array
	char[b]="xyz";
	char *c=&b[0]; // c store the address of start of address of b 
	
	//*c=&b[0]=&b=&b[];
	
	
	cout<< c<<endl;
	//prints xyz
			


return 0;
}

